{
  "name": "quizzey",
  "version": "1.0.0",
  "main": "index.js",
  "description": "GEA Backend API",
  "author": "Adewumi Alabi",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn src/index.ts",
    "build:prod": "cross-env NODE_ENV=production tsc && mkdir -p dist/mailer/views && cp -R src/mailer/views/*.hbs dist/mailer/views/",
    "start": "node dist/index.js",
    "migrate": "npx sequelize-cli db:migrate --env development",
    "mongo-migrate": "node src/migrations/mongo-migrate.js",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watchAll -i --detectOpenHandles"
  },
